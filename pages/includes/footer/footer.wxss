/*底部  */
.footer{
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    background-color: #ffffff;
    border-top: 2rpx solid #f5f5f5;
}
.footer-box{
    width: 25%;
    height: auto;
    text-align: center;
    padding: 10rpx 0;
    float: left;
}
.footer-box image{
    display: block;
    width: 40rpx;
    height: 40rpx;
    margin: 0 auto 10rpx;
}
.footer-box text{
    display: block;
    width: 100%;
    height: 40rpx;
    margin: 0 auto;
    text-align: center;
    color: #afafaf;
}
/** wxss **/
/** 修改默认的navigator点击态 **/
