
/**index.wxss**/
 /*头部*/
.head-top{
  position: relative;
  width: 100%;
  height: 300rpx;
}
.head-seacherdwbox{
  width: 100%;
  padding: 10rpx 20rpx;
  position: absolute;
  top: 10rpx;
  left: 0;
  height: 70rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  z-index: 10;
  box-sizing: border-box;
}
.head-site-box{
  display: inline-block;
  width: 16%;
}
.head-site{
  font-size: 26rpx;
  color: #fff;
  vertical-align: middle;
}
.head-arrow{
  width: 24rpx;
  height: 14rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.head-input-box{
  width: 63%;
  height: 60rpx;
  margin-left: 16rpx;
  display: inline-block;
  position: relative;
}
.head-input{
  padding-left: 65rpx;
  width:100%;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  border-radius: 15px;
  box-sizing: border-box;
  font-size: 26rpx;
  display: inline-block;
  vertical-align: middle;
  background:rgba(255,255,255,0.5);
}
.head-search-icon{
  position: absolute;
  width: 28rpx;
  vertical-align: middle;
  height: 28rpx;
  top: 20rpx;
  left: 17rpx;
}
.head-btn-box{
  width: 18%;
  float: right;
}
.index-iconhs{
  width: 42rpx;
  height: 42rpx;
  vertical-align: middle;
  margin-left: 10%;
}
.index-iconss{
  width: 35rpx;
  height: 42rpx;
  vertical-align: middle;
  margin-left: 10%;
}
 /*滚动banner*/
.banner-swiper{
     width:100%;
     height:300rpx;
 }

.banner-swiper image{
     width:100%;
     height:300rpx;
 }
 /*导航*/
.index-navbox{
    width: 100%;
    background: #fff;
    flex-direction:row;
    padding: 35rpx 35rpx 0 35rpx;
    box-sizing:border-box;
}
.index-nav{
   width: 20%;
   float: left;
   text-align: center;
   margin-bottom: 46rpx;
}
.index-navicon{
   width: 80rpx;
   height: 80rpx;
   display: block;
   margin: 0 auto;
}
.index-navtext{
   display: block;
   font-size: 24rpx;
   color: #333;
   margin-top: 18rpx;
}
  /*公告*/
.notice{
  background: #fff;
  padding: 0rpx 20rpx 10rpx 20rpx;
  flex-direction: row;
}
.notice-pic{
  width: 110rpx;
  height: 37rpx;
  vertical-align: middle;
  border-right: 1px solid #e6e6e6;
  padding-right: 16rpx;
}
.notice-swiper{
  width: 76%;
  height: 30rpx;
  margin-left: 20rpx;
  line-height: 30rpx;
  overflow: hidden;
  vertical-align: middle;
}
.notice-swiper text{
  font-size: 24rpx;
  vertical-align: middle;
  color: #333;
}
/*活动*/
.index-activity{
 width: 100%;
 background: #fff;
 overflow: hidden;
}
.index-activity-column{
  width: 50%;
  display: inline-block;
  overflow: hidden;
}
.activity-shop-coupon{
  padding: 30rpx 20rpx;
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  border-right: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
}
.shop-pic-icon{

}
.shop-text-introduce{
  width: 38%;
  margin-left: 10rpx;
  margin-top: -10rpx;
  position: relative;
  vertical-align: middle;
}
.shop-introduce-name{
  font-size: 24rpx;
  color: #333;
  width: 100%;
  height: 28rpx;
  line-height: 28rpx;
  display: inline-block;
  overflow: hidden;
}
.activity-shop-coupon .index-shop-pic{
  width: 60rpx;
  height: 60rpx;
  border-radius:50%;
  vertical-align: middle;
}
.small-stars{
  width: 109rpx;
  height: 16rpx;
  background-image: url("data:image/png;base64,");
  background-size: 218rpx 16rpx;
}
.small-stars.stars5{
  background-position: 0 0;
}
.small-stars.stars4{
  background-position: -23rpx 0;
}
.small-stars.stars3{
  background-position: -45rpx 0;
}
.small-stars.stars2{
  background-position: -67rpx 0;
}
.small-stars.stars1{
  background-position: -89rpx 0;
}
.small-stars.stars0{
  background-position: -110rpx 0;
}
.shop-yhcoupon{
  width: 32%;
  float: right;
  height: 24rpx;
  text-align: center;
  vertical-align: middle;
}
.money-coupon{
  color: #ff461c;
  width: 100%;
  display: inline-block;
  height: 50rpx;
  overflow: hidden;
  margin-top: -5rpx;
}
.money-symbol{
  font-size: 20rpx;
}
.money-num{
  font-size: 32rpx;
  font-weight: bold;
}
.receive-coupon{
  font-size: 18rpx;
  color: #999;
  display: inline-block;
  vertical-align: middle;
  margin-top: -62rpx;
}

.activity-box{
  padding:20rpx 30rpx;
  width: 100%;
  box-sizing: border-box;
  border-right:1px solid #e6e6e6;
  border-bottom:1px solid #e6e6e6;
}
.activity-box:nth-child(2) .blue-text{
    color: #4caae9;
}
.activity-text{
  width: 70%;
  overflow: hidden;
  vertical-align: middle;
}
.activity-text text{
  display: block;
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
  overflow: hidden;
}
.blue-text{
  font-weight: bold;
  font-size: 28rpx;
  color: #f95353;
}
.gary-text{
  color: #b2b2b2;
  font-size: 22rpx;
}
.black-text{
  color: #4c4c4c;
  font-size: 22rpx;
}
.activity-pic{
  width: 20%;
  vertical-align: middle;
}
.activity-icon{
  width: 102rpx;
  height: 102rpx;

}
.advertis{
  width: 100%;
  height: 140rpx;
}
/* 本地推荐 */
.recommend-title{
   background: #fff;
   width: 100%;
   border-bottom: 1px solid #e6e6e6;
   padding:10rpx 20rpx;
}
.recommend-icon{
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}
.recommend-title text{
  font-size: 28rpx;
  color: #2e2e2e;
  vertical-align: middle;
  margin-left: 12rpx;
}




.modal-box{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(0,0,0,0.5);
  overflow: hidden;
  z-index: 999;
  }

.modal-body{
  position:relative;
  }

.modal-content{
  margin:8% auto;
  overflow: hidden;
  }

.modal-content{
  width: 549rpx;
  height:auto;
}
.modal-content .red-packet{
 width: 549rpx;
 height: 706rpx;
 background-image: url("data:image/png;base64,");
 background-size: 100% 100%;
 text-align: center;
}
.red-packet-sum{
  color: #fe3638;
  font-size: 80rpx;
  margin-top: 134rpx;
  display: inline-block;
}
.red-packet-dw{
  font-size: 30rpx;
  margin-left: 10rpx;
}
.red-packet-detail{
  color: #ecc779;
  margin-top: 160rpx;
}
.red-packet-detail text{
  font-size: 40rpx;
}
.red-packet-text{
  font-size: 26rpx!important;
  width: 100%;
  display: inline-block;
}
.modal-content .knowBtn{
  width: 91rpx;
  height: 91rpx;
  margin: 10rpx auto 0 auto;
}
.modal-content .knowBtn image{
  width: 91rpx;
  height: 91rpx;
}
