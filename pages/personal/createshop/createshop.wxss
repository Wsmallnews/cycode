.create-table{
  background: #fff; 
  border-top: 1px solid #e6e6e6;
}
.create-tableb{
  background: #fff; 
  border-bottom: 1px solid #e6e6e6;
}
.create-tr{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding: 20rpx 20rpx;
  border-bottom: 1px solid #e6e6e6;
  position: relative;
}
.create-tr text{
  font-size: 28rpx;
  color: #333;
  width: 176rpx;
  display: inline-block;
}
.create-tr input{
  width: 100%;
  font-size: 28rpx;
}
.create-tr:last-child{
  border-bottom: none;
}
.triangle-right{
  width: 12rpx;
  height: 22rpx;
  position: absolute;
  right: 20rpx;
  top:50%;
  margin-top: -11rpx;
}
/* 身份 */
.sfcard-box{
  background: #fff;
  padding: 20rpx;
  width: 100%;
  box-sizing: border-box;
}
.sfcard-contentbox{
  width: 100%;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.sfcard{
  width: 50%;
  text-align: center;
  display: inline-block;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.upsfcard{
  width: 100%;
  height: 210rpx;
}
.upsfcard image.xy-pic{
  border-radius: 10rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.sfcard-text{
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  margin-top: 18rpx;
}
.license-box{
  width: 100%;
  text-align: center;
  display: inline-block;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.uplicense{
  width: 100%;
  height: 290rpx;
}
.uplicense image.xy-pic{
  border-radius: 10rpx;
  background-color: #fff;
  box-sizing: border-box;
}