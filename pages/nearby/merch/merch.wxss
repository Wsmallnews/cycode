.shop-banner-swiper{
     width:100%;
     height:380rpx;
 }

.shop-banner-swiper image{
     width:100%;
     height:380rpx;
 }

 .shop-detail-name{
   background: #fff;
   padding: 30rpx 20rpx;
   border-bottom: 1px solid #e6e6e6;
   overflow: hidden;
 }
 .shop-name-stars{
    width: 100%;
    position: relative;
 }
 .shop-name-box{
   width: 100%;
   overflow: hidden;
 }
 .shop-name-box text{
   font-size: 32rpx;
   color: #333;
   vertical-align: middle;
   margin-right: 10rpx;
   display: inline-block;
 }
 .shop-name-box image{
   width: 129rpx;
   height: 34rpx;
   vertical-align: middle;
 }
 .big-stars{
   width: 191rpx;
   display: inline-block;
   height: 29rpx;
   vertical-align: middle;
   background-image: url("data:image/png;base64,");
background-size: 395rpx 29rpx;
 }
 .big-stars.stars5{
   background-position: 0 0;
 }
 .big-stars.stars4{
   background-position: -40rpx 0;
 }
 .big-stars.stars3{
   background-position: -80rpx 0;
 }
 .big-stars.stars2{
   background-position: -120rpx 0;
 }
 .big-stars.stars1{
   background-position: -160rpx 0;
 }
 .big-stars.stars0{
   background-position: -202rpx 0;
 }
 .stars-fans text{
   color: #999;
   font-size: 24rpx;
   display: inline-block;
   margin-left: 15rpx;
   vertical-align: middle;
 }
 .shop-follow{
   position: absolute;
   right: 0;
   top: 10rpx;
   padding: 8rpx 14rpx;
   border: 1px solid #0096ec;
   border-radius: 10rpx;
   display: flex;
   flex-direction: row;
 }
 .shop-follow image{
   width: 30rpx;
   height: 28rpx;
   vertical-align: middle;
   margin-right: 12rpx;
 }
 .shop-follow text{
   color: #0096ec;
   font-size: 24rpx;
   vertical-align: middle;
   display: inline-block;
 }
 .shop-detail-site{
   display: flex;
   flex-direction: row;
   background: #fff;
   padding: 26rpx 20rpx;
 }
 .shop-detail-site image{
   width: 26rpx;
   height: 34rpx;
   vertical-align: middle;
 }
 .shop-detail-site text{
   font-size: 26rpx;
   color: #333;
   width: 95%;
   margin-left: 16rpx;
   vertical-align: middle;
   display: inline-block;
 }
 .shop-map-tel{
   background: #fff;
   padding: 20rpx;
   width: 100%;
 }
 .shop-icontext{
   width: 49.5%;
   border-right: 1px solid #e6e6e6;
   text-align: center;
   display: inline-block;
   float: left;
 }
 .shop-icontext image{
   width: 50rpx;
   height: 40rpx;
 }
 .shop-icontext text{
   display: block;
   color: #666;
   font-size: 24rpx;
 }
 .shop-list{
   width: 100%;
   background: #fff;

 }
 .shop-list-box{
   padding: 24rpx 20rpx;
   border-bottom: 1px solid #e6e6e6;
   display: flex;
   flex-direction: row;
 }
 .vip-zhekou{
   font-size: 26rpx;
   color: #999;
   vertical-align: middle;
 }
 .shop-list-box:last-child{
   border-bottom: 0;
 }
 .shop-bqlabel{
   width: 104rpx;
   height: 32rpx;
   margin-right: 20rpx;
 }

 .uesr-evaluate{
    background: #fff;
 }
 .uesr-evaluate-tit{
   padding: 20rpx;
   border-bottom: 1px solid #e6e6e6;
   display: flex;
   flex-direction: row;
 }
 .uesr-evaluate-tit image{
   width: 32rpx;
   height: 32rpx;
   margin-right: 10rpx;
 }